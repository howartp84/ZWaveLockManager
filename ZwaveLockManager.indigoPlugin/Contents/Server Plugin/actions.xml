	<Action id="setRTC" deviceFilter="self.doorLock">
		<Name>Set Clock</Name>
		<CallbackMethod>setRTC</CallbackMethod>
	</Action>

	<Action id="setRelock" deviceFilter="self.doorLock">
		<Name>Set Auto Relock</Name>
		<CallbackMethod>setRelock</CallbackMethod>
		<ConfigUI>
			<Field id="relockOn" type="menu" defaultValue="on">
				<Label>Auto relock after a code has been entered:</Label>
				<List>
					<Option value="on">On</Option>
					<Option value="off">Off</Option>
				</List>
			</Field>
			<Field id="relockTime" type="textfield" defaultValue="30" visibleBindingId="relockOn" visibleBindingValue="on">
				<Label>Number of seconds before door relocks:</Label>
			</Field>
		</ConfigUI>
	</Action>

	<Action id="setWrongLimit" deviceFilter="self.doorLock">
		<Name>Set Wrong Code Limit and Timeout</Name>
		<CallbackMethod>setWrongLimit</CallbackMethod>
		<ConfigUI>
			<Field id="wrongCount" type="menu" defaultValue="5">
				<Label>Number of incorrect codes before reporting to Indigo:</Label>
				<List>
					<Option value="1">1</Option>
					<Option value="2">2</Option>
					<Option value="3">3</Option>
					<Option value="4">4</Option>
					<Option value="5">5</Option>
					<Option value="6">6</Option>
					<Option value="7">7</Option>
				</List>
			</Field>
			<Field id="shutdownTime" type="textfield" defaultValue="60">
				<Label>Number of seconds that lock is inoperable after reaching limit:</Label>
			</Field>
		</ConfigUI>
	</Action>

	<Action id="setMode" deviceFilter="self.doorLock">
		<Name>Set Operating Mode</Name>
		<CallbackMethod>setMode</CallbackMethod>
		<ConfigUI>
			<Field id="modeVal" type="menu" defaultValue="normal">
				<Label>Set operating mode to:</Label>
				<List>
					<Option value="0">Normal</Option>
					<Option value="1">Vacation Mode</Option>
					<Option value="2">Privacy Mode</Option>
				</List>
			</Field>
		</ConfigUI>
	</Action>
	
	<Action id="setModeIDLock" deviceFilter="self.doorLock"> <!--ID Lock AwayMode is Param 1)-->
		<Name>Set Operating Mode (ID Lock 100/150)</Name>
		<CallbackMethod>setModeIDLock</CallbackMethod>
		<ConfigUI>
			<Field id="modeVal" type="menu" defaultValue="normal">
				<Label>Set operating mode to:</Label>
				<List>
					<Option value="0">Home - Manual Lock</Option>
					<Option value="1">Home - Auto Lock</Option>
					<Option value="2">Away - Manual Lock</Option>
					<Option value="3">Away - Auto Lock</Option>
				</List>
			</Field>
		</ConfigUI>
	</Action>

	<Action id="getModeIDLock" deviceFilter="self.doorLock">
		<Name>Get Lock Mode (ID Lock 100/150 Only)</Name>
		<CallbackMethod>getModeIDLock</CallbackMethod>
	</Action>

</Actions>
